# GitLab配置
GITLAB_URL=https://your-gitlab-instance.com
GITLAB_TOKEN=your_private_token
GITLAB_API_VERSION=v4
GITLAB_TIMEOUT=30000

# 服务器配置
PORT=3000
NODE_ENV=development
BODY_LIMIT=1mb
TRUSTED_PROXIES=127.0.0.1,::1

# 日志配置
LOG_LEVEL=info
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# 安全配置
ENABLE_STRICT_CORS=false
ALLOWED_ORIGINS=*
RATE_LIMIT=100 